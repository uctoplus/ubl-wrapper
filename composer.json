{
  "name": "uctoplus/ubl-wrapper",
  "description": "A PHP wrapper for UBL documents",
  "keywords": [
    "ubl",
    "parser",
    "generator",
    "uctoplus",
    "document",
    "invoice",
    "order",
    "packing list",
    "quotation",
    "credit note",
    "debit note"
  ],
  "type": "library",
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Uctoplus\\UblWrapper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Uctoplus\\UblWrapper\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "uctoplus.sk",
      "email": "info@uctoplus.sk"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-dom": "*",
    "ext-zip": "*",
    "ext-libxml": "*",
    "greenter/ubl-validator": "^2.1",
    "nesbot/carbon": "^3.8"
  },
  "require-dev": {
    "ext-simplexml": "*",
    "ext-json": "*",
    "phpunit/phpunit": "^11.0",
    "symfony/var-dumper": "^7.2",
    "symfony/console": "^7.2"
  }
}
